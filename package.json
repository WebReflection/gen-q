{
  "name": "gen-q",
  "version": "0.1.5",
  "description": "An efficient queue implementation for JavaScript",
  "main": "index.js",
  "module": "index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./types/index.d.ts"
    },
    "./utils": {
      "import": "./utils.js",
      "types": "./types/utils.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "index.js",
    "utils.js",
    "README.md",
    "LICENSE",
    "types/*.d.ts"
  ],
  "scripts": {
    "build": "npm run types",
    "coverage": "mkdir -p ./coverage; c8 report --reporter=text-lcov > ./coverage/lcov.info",
    "test": "c8 node test.js",
    "types": "rm -rf types && tsc --allowJs --checkJs --lib esnext --moduleResolution nodenext --module NodeNext --target esnext -d --emitDeclarationOnly --outDir ./types ./index.js ./utils.js"
  },
  "keywords": [
    "generator",
    "queue",
    "async",
    "array"
  ],
  "author": "Andrea Giammarchi",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "c8": "^10.1.3",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebReflection/gen-q.git"
  },
  "bugs": {
    "url": "https://github.com/WebReflection/gen-q/issues"
  },
  "homepage": "https://github.com/WebReflection/gen-q#readme"
}
